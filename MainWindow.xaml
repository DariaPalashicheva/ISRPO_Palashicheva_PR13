<Window x:Class="ISRPO_Palashicheva_PR13.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ISRPO_Palashicheva_PR13"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition/>
            <RowDefinition Height="70"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="#FF54F5F9"/>
        <Image Source="Resources/icon.png" HorizontalAlignment="Left" Height="60" Width="60"/>
        <TextBlock Text="Склад" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" FontSize="40"/>
        <Frame Grid.Row="1" x:Name="frmMain" NavigationUIVisibility="Hidden"/>
        <Rectangle Fill="#FF54F5F9" Grid.Row="2"/>
    </Grid>
</Window>
